def joke1():
    print("from the screen, to the ring, to the pen, to the king.")

def joke2():
    print("I didn't have lunch.")

def joke3():
    print("what a joke")

joke = int(input("which joke that you want to hear 1-3: "))
if joke == 1:
    joke1()
elif joke == 2:
    joke2()
elif joke == 3:
    joke3()
else:
    print("what are joke to chose a wrong anser even only have three choices")
